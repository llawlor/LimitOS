<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<meta name="version" content="<%= GIT_VERSION %>">
<title><%= content_for?(:title) ? yield(:title) : Rails.application.config_for(:limitos)['application_name'] %></title>
<link rel="icon" href="<%= image_url('favicon.png') %>">
<%= csrf_meta_tags %>

<link href="https://fonts.googleapis.com/css?family=Libre+Franklin" rel="stylesheet">
<%= stylesheet_link_tag "application", :media => "all" %>
<%= stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/bootstrap-slider/9.7.2/css/bootstrap-slider.min.css' %>
<%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/bootstrap-slider/9.7.2/bootstrap-slider.min.js' %>

<%= javascript_include_tag "application" %>

<!-- Le HTML5 shim, for IE6-8 support of HTML elements -->
<!--[if lt IE 9]>
<script src="//cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.2/html5shiv.min.js" type="text/javascript"></script>
<![endif]-->
